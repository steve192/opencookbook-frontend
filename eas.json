{
  "cli": {
    "version": ">= 0.35.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "preview",
      "android": {
        "distribution": "internal",
        "withoutCredentials": true,
        "buildType": "apk"
      },
      "env": {
        "EX_UPDATES_NATIVE_DEBUG": "1"
      },
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "APP_VARIANT": "preview"
      }
    },
    "production": {
      "channel": "production"
    },
    "production-apk": {
      "channel": "production",
      "android": {
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {}
  }
}

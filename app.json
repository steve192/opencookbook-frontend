{
  "expo": {
    "name": "opencookbook",
    "slug": "opencookbook",
    "scheme": "opencookbook",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "updateUrl": "https://expo.dev/@steve192/opencookbook",
      "sdkVersion": 43,
      "releaseChannel": "default",
      "checkOnLaunch": "NEVER"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "buildNumber": "0.0.1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.sterul.opencookbook",
      "permissions": [
        "CAMERA"
      ],
      "versionCode": 5
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}